    {
        type    calculated;
        value   uniform 0;
    }

    outlet
    {
        type    calculated;
        value   uniform 0;
    }

    topwall
    {
        type    calculated;
        value   uniform 0;
    }

    bottomwall
    {
        type    calculated;
        value   uniform 0;
    }

    leftwall
    {
        type    calculated;
        value   uniform 0;
    }

    rightwall
    {
        type    calculated;
        value   uniform 0;
    }

    cylinder
    {
        type    calculated;
        value   uniform 0;
    }
}


