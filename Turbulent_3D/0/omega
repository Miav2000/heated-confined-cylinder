FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 637.7170883;   // omega for Re = 4Â·10^5;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 637.7170883;
    }

    outlet
    {
        type            zeroGradient;
    }

    topwall
    {
        type            omegaWallFunction;
        value           uniform 1e8;
    }

    bottomwall
    {
        type            omegaWallFunction;
        value           uniform 1e8;
    }

    leftwall
    {
        type            omegaWallFunction;
        value           uniform 1e8;
    }

    rightwall
    {
        type            omegaWallFunction;
        value           uniform 1e8;
    }

    cylinder
    {
        type            fixedValue;
        value           uniform 1e8;
    }
}


